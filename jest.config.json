{
    "collectCoverage": true,
    "collectCoverageFrom": [
        "src/**/*.js*"
    ],
    "coveragePathIgnorePatterns": [
        "node_modules",
        "src/index.js",
        "src/config.js",
        "src/init.js",
        "src/metrics.js",
        "src/logger.js"
    ],
    "coverageReporters": [
        "json-summary",
        "text"
    ],
    "coverageThreshold": {
        "global": {
            "lines": 80
        }
    }
}